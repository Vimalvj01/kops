load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = [
        "config.go",
        "types.go",
        "version.go",
    ],
    importmap = "k8s.io/kops/vendor/github.com/spotinst/spotinst-sdk-go/spotinst",
    importpath = "github.com/spotinst/spotinst-sdk-go/spotinst",
    visibility = ["//visibility:public"],
    deps = [
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/credentials:go_default_library",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/log:go_default_library",
    ],
)

filegroup(
    name = "package-srcs",
    srcs = glob(["**"]),
    tags = ["automanaged"],
    visibility = ["//visibility:private"],
)

filegroup(
    name = "all-srcs",
    srcs = [
        ":package-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/client:all-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/credentials:all-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/log:all-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/session:all-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/util/jsonutil:all-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/util/stringutil:all-srcs",
        "//vendor/github.com/spotinst/spotinst-sdk-go/spotinst/util/uritemplates:all-srcs",
    ],
    tags = ["automanaged"],
    visibility = ["//visibility:public"],
)
